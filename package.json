  },
  "homepage": "https://github.com/eydrian/eslint-vertical-import#readme",
  "devDependencies": {
    "@types/node": "^12.19.12",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
